{
  "name": "wp-theme-starter",
  "version": "0.5.0",
  "description": "Website - WordPress Theme",
  "repository": "",
  "author": "CAMP4 inc.",
  "license": "UNLICENSED",
  "private": true,
  "engines": {
    "node": "16"
  },
  "scripts": {
    "dev": "npm run dev --workspaces --if-present",
    "build": "npm run build --workspaces --if-present",
    "prettier": "npm run prettier --workspaces --if-present",
    "wp-setup": "docker-compose up -d && sleep 7 && docker-compose run --rm --user root cli /html/setup.sh",
    "wp-cli": "zx ./docker/bin/wp-cli.mjs",
    "wp-wordmove": "zx ./docker/bin/wp-wordmove.mjs"
  },
  "devDependencies": {
    "zx": "^5.1.0"
  },
  "workspaces": [
    "mytheme"
  ]
}
